        function ValidatePageDoctorate(oSrc, args) {
            var txtFrom = document.getElementById('txtDocFrom').value;
            var txtTo = document.getElementById('txtDocTo').value;
            var now = new Date();

            var dFrom = Date.parse(txtFrom);
            var dTo = Date.parse(txtTo);

            document.getElementById('hfCounter').Value = "0";

            if (dFrom < now && dTo < now) {
                if (dFrom >= dTo) {
                    args.IsValid = false;
                }
            }
