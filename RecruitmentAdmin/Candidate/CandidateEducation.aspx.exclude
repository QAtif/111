                                                                                        label: item.Name,
                                                                                        value: item.Name,
                                                                                        itemid: item.Code
                                                                                    }
                                                                                }))
                                                                            },
                                                                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                                                                alert(textStatus);
                                                                            }
                                                                        });
                                                                    },
                                                                    minLength: 2,
                                                                    select: function (event, ui) {
                                                                        $('#hfMatricField').val(ui.item.itemid);
                                                                    },
                                                                    change: function (event, ui) {
                                                                        if (ui.item == null) {
                                                                            $('#hfMatricField').val("0");
                                                                        }
                                                                    }
                                                                });

                                                            });
